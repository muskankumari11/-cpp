#include <iostream>
#include<vector>
using namespace std;
int main() {
	vector<int>v;
cout<<"capacity->"<<v.capacity()<<endl;
v.push_back(1);
cout<<"capacity->"<<v.capacity()<<endl;
v.push_back(2);
cout<<"capacity->"<<v.capacity()<<endl;
v.push_back(3);
cout<<"capacity->"<<v.capacity()<<endl;
cout<<"2nd index size->"<<v.at(1)<<endl;
cout<<"size->"<<v.size()<<endl;
//pop
cout<<"before pop"<<endl;
for(int i:v){
cout<<i<<" ";
cout<<endl;
}
v.pop_back();
cout<<"after pop"<<endl;
for(int i:v){
cout<<i<<" ";
}
}
