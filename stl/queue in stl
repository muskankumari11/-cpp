#include <iostream>
#include<queue>
using namespace std;
int main() {
queue<string>q;
q.push("hello");
q.push("miss");
q.push("mk");
cout<<"size before pop->"<<q.size()<<endl;
cout<<"first element->"<<q.front()<<endl;
q.pop();
cout<<"front element->"<<q.front()<<endl;
cout<<"size of queue->"<<q.size();
cout<<endl;
cout<<"is empty->"<<q.empty()<<endl;
}
